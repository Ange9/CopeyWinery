@model IEnumerable<CopeyWinery.Models.Lane>

@{
    ViewBag.Title = "Index";
}
<link rel="stylesheet" href="\fonts\tableStyle.css">

<h2>Index</h2>

<p>
    @Html.ActionLink("Agregar", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.plant_quantity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.plant_quantity)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id_lane }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id_lane }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id_lane })
        </td>
    </tr>
}

</table>
